    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
